<?xml version="1.0" encoding="utf-8"?>
<tar>
    <Description>Create an archive or decompress an archive</Description>
    <Example>
        <ExampleCommand>1. tar xzvf file.tar.gz</ExampleCommand>
        <ExampleDetails>Decompress the file.tar.gz archive</ExampleDetails>

    </Example>
    <Example>
        <ExampleCommand>2. tar cjvf file.tar.gz file1 file2</ExampleCommand>
        <ExampleDetails>Create 'file.tar.gz', a zipped version of 'file1' and 'file2'</ExampleDetails>
    </Example>
</tar>