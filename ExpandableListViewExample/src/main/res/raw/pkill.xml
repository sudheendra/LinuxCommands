<?xml version="1.0" encoding="utf-8"?>
<pkill>
    <Description>kills a process by name </Description>
    <Example>
        <ExampleCommand>1. pkill NAME</ExampleCommand>
        <ExampleDetails>Kill all processes with NAME in their name</ExampleDetails>
    </Example>
    <Example>
        <ExampleCommand>2. pkill -9 NAME</ExampleCommand>
        <ExampleDetails>For REALLY stubborn processes</ExampleDetails>
    </Example>
</pkill>