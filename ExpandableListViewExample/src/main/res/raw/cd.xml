<?xml version="1.0" encoding="utf-8"?>
<cd>
    <Description>change working directory</Description>
    <Example>
        <ExampleCommand>1. cd..</ExampleCommand>
        <ExampleDetails>change to parent directory</ExampleDetails>
    </Example>
    <Example>
        <ExampleCommand>2. cd ~/Public</ExampleCommand>
        <ExampleDetails>change to /home/USERNAME/Public directory</ExampleDetails>
    </Example>
    <Example>
        <ExampleCommand>3. cd -</ExampleCommand>
        <ExampleDetails>change to previous directory</ExampleDetails>
    </Example>
</cd>
