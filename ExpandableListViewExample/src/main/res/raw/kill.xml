<?xml version="1.0" encoding="utf-8"?>
<kill>
    <Description>Kills a process</Description>
    <Example>
        <ExampleCommand>1. kill PID</ExampleCommand>
        <ExampleDetails>Kill a process with process id PID</ExampleDetails>
    </Example>
    <Example>
        <ExampleCommand>2. kill -9 PID</ExampleCommand>
        <ExampleDetails>For stubborn processes, REALLY kill them.</ExampleDetails>
    </Example>
</kill>